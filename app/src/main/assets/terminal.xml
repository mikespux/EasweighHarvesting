<?xml version="1.0" encoding="utf-8"?><!--
        WARNNING!!!!
        DO NOT MODIFY THIS FILE UNTIL YOU GET SUPPORT!
-->
<terminal>
    <!--
      ANDROID_LOG_UNKNOWN = 0
      ANDROID_LOG_DEFAULT = 1
      ANDROID_LOG_VERBOSE = 2
      ANDROID_LOG_DEBUG   = 3
      ANDROID_LOG_INFO    = 4
      ANDROID_LOG_WARN    = 5
      ANDROID_LOG_ERROR   = 6
      ANDROID_LOG_FATAL   = 7
      ANDROID_LOG_SILENT  = 8
    -->
    <use-sdk loglevel="5" version="1.0" />

    <device forced="true" model="auto-detect" type="fingerprint_sensor">
        <power type="on">
            <pmic on="true" pin="VGP3" voltage="1800" />
            <gpio direction="output" pin="84" value="1" />
            <gpio direction="output" pin="18" value="1" />
            <delay timems="100" />
            <gpio direction="output" pin="16" value="0" />
            <delay timems="200" />
            <gpio direction="output" pin="19" value="1" />
            <delay timems="3000" />
        </power>

        <power type="off">
            <gpio direction="output" pin="19" value="0" />
            <gpio direction="input" pin="16" />
            <gpio direction="output" pin="18" value="0" />
            <gpio direction="output" pin="84" value="0" />
            <pmic on="false" pin="VGP3" />
            <delay timems="500" />
        </power>
    </device>

    <device forced="false" model="auto-detect" type="idcard_reader">
        <uart baudrate="115200" path="/dev/ttyS0" />
    </device>

    <device forced="true" model="em2028" type="hw_barcode">
        <power type="on">
            <pmic on="true" pin="VGP3" voltage="1800" />
            <pmic on="true" pin="VIBR" voltage="3300" />
            <pmic on="true" pin="ISINK2" voltage="1800" />
            <delay timems="1000" />
        </power>

        <power type="off">
            <pmic on="false" pin="VIBR" />
            <pmic on="false" pin="ISINK2" />
            <pmic on="false" pin="VGP3" />
        </power>

        <power type="active_uart">
            <gpio direction="output" pin="82" value="0" />
        </power>

        <uart baudrate="9600" path="/dev/ttyMT1" />
    </device>
</terminal>